#include <stdio.h>
int main()
{
    float Sum,Avg,C,P,M,E,CS,P;
    printf("Enter Chemistry Marks :");
    scanf("%f",&C);
    printf("Enter Physics Marks :");
    scanf("%f",&P);
    printf("Enter Maths Marks :");
    scanf("%f",&M);
    printf("Enter English Marks :");
    scanf("%f",&E);
    printf("Enter Computer Science Marks :");
    scanf("%f",&CS);
    Sum=C+P+M+E+CS;
    P=(Sum/500)*100;
    Avg=Sum/5;
    printf("\nTotal marks: %.2f",Sum);
    printf("\nAverage : %.2f",Avg);
    printf("\nPercentage : %.2f",P);
    return 0;
}
